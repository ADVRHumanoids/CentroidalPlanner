<launch>

    <arg name="rate" default="100.0"/>
    <arg name="prefix" default=""/>
    
    <param name="robot_description" 
        textfile="$(env ROBOTOLOGY_ROOT)/external/CentroidalPlanner/template-ros-pkg/urdf/template.urdf"/>
        
    <param name="robot_description_semantic"
        textfile="$(env ROBOTOLOGY_ROOT)/external/CentroidalPlanner/template-ros-pkg/srdf/template.srdf"/>
        
    <param name="cartesian/problem_description" 
        textfile="$(find centroidal_planner)/cartesio_stack/template_stack.yaml"/>
        
    <include file="$(find cartesian_interface)/launch/cartesio.launch">
        <arg name="rate" value="$(arg rate)"/>
        <arg name="prefix" value="$(arg prefix)"/>
    </include>      

</launch>